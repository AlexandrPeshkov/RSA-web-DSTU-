@model ResultView
@{
    ViewData["Title"] = "Результат";
    Layout = "_Layout";
}

@using (Html.BeginForm())
{
    @Html.Partial("Partial/_Configuration", Model.Configuration)

    @Html.Partial("Partial/_Point", Model.ZeroPoint)

    @Html.Partial("Partial/_Directions", Model?.Directions)

    @Html.Partial("Partial/_Steps", Model.Steps)

    @Html.Partial("Partial/_Step", Model.BestSolution)
}

