@model ConfigurationView
@{
    ViewData["Title"] = "Конфигурация";
    Layout = "_Layout";
}

@using (Html.BeginForm())
{
    @Html.AntiForgeryToken();
    @Html.ValidationSummary(true)

    @Html.Partial("Partial/_Configuration", Model)

    @Html.Partial("Partial/_Buttons", new ButtonsView(Buttons.ApplyButton))
}